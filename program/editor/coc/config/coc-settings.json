{
  "coc.preferences.colorSupport": false,
  "coc.preferences.formatOnSaveFiletypes": ["lua", "python", "rust"],
  "coc.preferences.rootPatterns": [".git", ".hg", ".projections.json", ".root"],
  "suggest.numberSelect": false,
  "suggest.maxCompleteItemCount": 15,
  "suggest.triggerCompletionWait": 30,
  "suggest.completionItemKindLabels": {
    "keyword": "\uf1de",
    "variable": "\ue79b",
    "value": "\uf89f",
    "operator": "\u03a8",
    "function": "\u0192",
    "reference": "\ufa46",
    "constant": "\uf8fe",
    "method": "\uf09a",
    "struct": "\ufb44",
    "class": "\uf0e8",
    "interface": "\uf417",
    "text": "\ue612",
    "enum": "\uf435",
    "enumMember": "\uf02b",
    "module": "\uf40d",
    "color": "\ue22b",
    "property": "\ue624",
    "field": "\uf9be",
    "unit": "\uf475",
    "event": "\ufacd",
    "file": "\uf723",
    "folder": "\uf114",
    "snippet": "\ue60b",
    "typeParameter": "\uf728",
    "default": "\uf29c"
  },
  "codeLens.enable": false,
  "markdownlint.config": { "line-length": false },
  "Lua.runtime.version": "LuaJIT",
  "Lua.diagnostics.globals": ["use", "vim", "vimp"],
  "python.analysis.typeCheckingMode": "off",
  "python.formatting.provider": "black",
  "python.linting.flake8Enabled": false,
  "pyright.organizeimports.provider": "isort",
  "snippets.textmateSnippetsRoots": ["~/dev/snippets"],
  "yaml.schemas": {
    "/Users/lgtf/git/sc-deploy/deploy/schemas/core-data-snapshotter/schema.json": [
      "/core_data_snapshotter.yml"
    ]
  },
  "languageserver": {
    "bash": {
      "command": "bash-language-server",
      "args": ["start"],
      "filetypes": ["sh"],
      "ignoredRootPaths": ["~"]
    },
    "ccls": {
      "command": "ccls",
      "filetypes": ["c", "cpp", "objc", "objcpp"],
      "rootPatterns": [".ccls", "compile_commands.json", ".git/", ".hg/"],
      "initializationOptions": {
        "cache": {
          "directory": "/tmp/ccls"
        }
      }
    },
    "clojure-lsp": {
      "command": "clojure-lsp",
      "filetypes": ["clojure"],
      "rootPatterns": ["project.clj", ".git/"],
      "additionalSchemes": ["jar", "zipfile"]
    },
    "dockerfile": {
      "command": "docker-langserver",
      "args": ["--stdio"],
      "filetypes": ["dockerfile"]
    },
    "efm": {
      "command": "efm-langserver",
      "args": [],
      "filetypes": ["*"]
    },
    "haskell": {
      "command": "hie-wrapper",
      "args": ["--lsp"],
      "rootPatterns": ["stack.yaml", "cabal.config", "package.yaml"],
      "filetypes": ["hs", "lhs", "haskell"],
      "initializationOptions": {
        "languageServerHaskell": {
          "hlintOn": true
        }
      }
    },
    "neuron": {
      "command": "neuron-language-server",
      "filetypes": ["markdown"]
    },
    "nix": {
      "command": "rnix-lsp",
      "filetypes": ["nix"]
    },
    "sourcery": {
      "command": "sourcery",
      "args": ["lsp"],
      "filetypes": ["python"],
      "initializationOptions": {
        "token": "<XXX>"
        "extension_version": "coc.vim",
        "editor_version": "vim"
      },
      "settings": {
        "sourcery": {
          "token": "<XXX>"
          "metricsEnabled": true
        }
      }
    }
  }
}
